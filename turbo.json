{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "dev": {
      "dependsOn": ["@openformat/sdk#build", "@openformat/react#build"]
    },
    "@openformat/sdk#build": {
      "dependsOn": ["generate-contract-types"]
    },
    "generate-contract-types": {},
    "start": {
      "cache": false
    }
  }
}
